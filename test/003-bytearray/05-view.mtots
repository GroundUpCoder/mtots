
final ba = ByteArray('\xF1\xF2\xF3\xF4\xF5\xF6\xF7')
print('ord(ba.raw()[0]) = %s' % [ord(ba.raw()[0])])
print('repr(ba.raw()) = %s' % [repr(ba.raw())])

final view1 = ba.view(2, -2)
print('repr(view1.raw()) = %s' % [repr(view1.raw())])

view1[0] = 0xF0
print('repr(view1.raw()) = %s' % [repr(view1.raw())])
print('repr(ba.raw()) = %s' % [repr(ba.raw())])

final view2 = view1.view(2)
print('repr(view2.raw()) = %s' % [repr(view2.raw())])

view2[-1] = 0xFA
print('repr(view2.raw()) = %s' % [repr(view2.raw())])
print('repr(view1.raw()) = %s' % [repr(view1.raw())])
print('repr(ba.raw()) = %s' % [repr(ba.raw())])

